{
    "manifest_version": 2,
    "name": "Password Maker",
    "description": "A SuperGenPass implementation",
    "version": "0",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["js/content.js"]
    }],
    "permissions": ["tabs"]
}
